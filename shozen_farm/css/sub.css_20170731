}
.eraberu_area03{
	padding-top:40px;
	padding-bottom:40px;
}
.eraberu_area03 .right_area{
	float:right;
	width:53%;
}
.eraberu_area03 .right_area img{
	width:100%;
}
.eraberu_area03 .left_area{
	float:left;
	width:47%;
	padding-top: 3%;
