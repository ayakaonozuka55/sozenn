/* ------------------------------------------------------------------
 NEWS
------------------------------------------------------------------ */
.pager_top {
  width: 100%;
  text-align: center;
}

.pager_top .pager_list {
  display: inline-block;
  margin: 40px auto 0;
}

.pager_top li {
  border: 2px solid #DB8E35;
  display: inline-block;
  float: left;
  font-size: 18px;
  text-align: center;
}

.pager_top li a {
  color: #DB8E35;
  padding: 2px 10.5px;
}

.pager_top li a:hover {
  background-color: #DB8E35;
  color: #fff;
}

.pager_top .current a {
  color: #fff;
  padding: 2px 10.5px;
}

.pager_top .page-numbers  {
  padding: 6px 10.5px;
  display: block;
}

.pager_top li:nth-child(n+2) {
  margin-left: 10px;
}

.pager_top .current {
  background-color: #DB8E35;
  color: #fff;
}

.pager_top .return {
  margin-right: 20px;
}

.pager_top li.next {
  margin-left: 20px;
}

.pager_top .return a,
.pager_top .next a {
  font-size: 13px;
  padding: 6px 10.5px;
}
