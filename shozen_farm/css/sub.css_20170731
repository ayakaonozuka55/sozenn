    background-repeat: no-repeat;
    vertical-align: middle;
    position: absolute;
    z-index: 2;
    width: 215px;
    height: 115px;
    margin-top: -7%;
    margin-left: 2%;
    padding: 2% 0;
    line-height: 2.5rem;
    text-align: center;
	color:#DB8E35;
    font-weight: bold;
}
.ttl_oneline{
    padding-top: 3%;
}
.eraberu_area01{
	padding-bottom:40px;
	border-bottom:1px solid #ddd;
}
.eraberu_area01 .right_area{
	float:right;
	width:53%;
}
.eraberu_area01 .right_area img{
	width:100%;
}
.eraberu_area01 .left_area{
	float:left;
	width:47%;
	padding-top: 3%;
	padding-right: 1%;
}
.eraberu_ttl{
    font-size: 2.8rem;
    font-weight: bold;
}
.eraberu_txt{
    line-height: 3rem;
}
.line_label{
	position: relative;
    padding-bottom: 20px;
    padding-left: 50px;
    margin-bottom: 30px;
	line-height: 3.5rem;
}
.line_label::after{
    content: '';
    width: 35px;
    height: 2px;
    background-color: #DB8E35;
    position: absolute;
    left: 0;
    top: 15px;
}
.eraberu_area02{
	padding:40px 0;
	border-bottom:1px solid #ddd;
}
.eraberu_area02 .right_area{
	float:right;
	width:47%;
