.wrapper_widesub {
    width: 100%;
    margin: 0 auto;
    position: relative;
    height: 100% !important;
    overflow: hidden;
}
.pankuzu {
	max-width: 1200px;
    width: 100%;
    margin: 0 auto;
    background-color: #fff;
	color:#000;
    position: relative;
    margin-top: 55px;
    font-size: 1.3rem;
}
.pankuzu a{
	color:#DB8E35;
}
.pankuzu a:hover{
	color:#DB8E35;
	text-decoration: underline;
}
.subttl_area {
    width: 100%;
